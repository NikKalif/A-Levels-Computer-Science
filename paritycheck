bytee = ["10101010", "00001101"]

def paritycheck(byte):
    count = 0
    for i in byte:
        for a in i:
            print(a)
            if a == "1":
                count += 1
            else:
                continue
        if count%2 == 0:
            print("The Parity Byte is 0")
        else:
            print("The Parity Byte is 1")
        count = 0

paritycheck(bytee)
